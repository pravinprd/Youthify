<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.srikrishnan.youthify">
<uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Dark"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUp"
            android:theme="@style/AppTheme.Dark"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".CoreActivity"
            android:label="@string/title_activity_core"
            android:theme="@style/AppTheme2"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".PostActivity"
            android:theme="@style/AppTheme2"
            android:parentActivityName=".CoreActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".InvolvedActivity"
            android:label="@string/title_activity_involved"
            android:parentActivityName=".CoreActivity"
            android:theme="@style/AppTheme2"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".InterestedActivity"
            android:label="@string/title_activity_interested"
            android:theme="@style/AppTheme2"
            android:parentActivityName=".CoreActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".EditProfileActivity"
            android:theme="@style/AppTheme2"
            android:parentActivityName=".CoreActivity"
            android:screenOrientation="portrait"/>
    </application>

</manifest>
